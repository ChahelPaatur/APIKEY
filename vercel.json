{
  "version": 2,
  "builds": [
    {
      "src": "api/predict.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/predict",
      "dest": "api/predict.py"
    },
    {
      "src": "/api/health",
      "dest": "api/health.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "functions": {
    "api/*.py": {
      "memory": 3008,
      "maxDuration": 60
    }
  }
}

